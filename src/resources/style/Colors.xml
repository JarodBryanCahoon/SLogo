<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Colors>
	<Base>
		<root>
			<fx-font>14px 'Letter Gothic Std'</fx-font>	
			<fx-text-fill>white</fx-text-fill>
			<fx-background-color>black</fx-background-color>
			<fx-wrap-text>true</fx-wrap-text>
		</root>
	</Base>
	
	<Word>
		<Variable>
			<fx-fill>#ccffffff</fx-fill>
		</Variable>
		<Constant>
			<fx-fill>pink</fx-fill>
		</Constant>
		<Command>
			<fx-fill>#99cc99ff</fx-fill>
		</Command>
		<Text>
			<fx-fill>#e6e6e6ff</fx-fill>	
		</Text>
		<Invalid>
			<fx-fill>#e6e6e6ff</fx-fill>	
		</Invalid>
	</Word>
	
	<Windows>
		<Window>
			<fx-background-color>#333333ff</fx-background-color>
			
			<fx-text-fill>white</fx-text-fill>
		</Window>
		<Window.content>
			<fx-background-color>#333333ff</fx-background-color>
		</Window.content>
	</Windows>
	
	<Rendering>
		
		<Render>
			<fx-background-color>#000000ff</fx-background-color>
			<fx-stroke>#ffffffff</fx-stroke>
			<fx-width>10</fx-width>
			<turtle>red</turtle>
		</Render>
	</Rendering>
	
	<Scroll>
		<ScrollPane>
			<fx-background-color>#333333ff</fx-background-color>
			<fx-background>BLACK</fx-background>
		</ScrollPane>
			
		<ScrollPane.decrement-button>
			<visibility>hidden</visibility>
		</ScrollPane.decrement-button>
	</Scroll>
	<Colorpickering>
		<ColorPicker>
			<fx-font>8px 'Letter Gothic Std'</fx-font>
			<fx-background-color>white</fx-background-color>
		</ColorPicker>
	</Colorpickering>

</Colors>
