<?xml version="1.0" encoding="UTF-8"?>
<Colors>
	<Base>
		<root>
			<fx-font>14px 'Letter Gothic Std'</fx-font>	
			<fx-text-fill>white</fx-text-fill>
			<fx-background-color>black</fx-background-color>
			<fx-wrap-text>true</fx-wrap-text>
		</root>
	</Base>
	
	<Word>
		<Variable>
			<fx-fill>greenyellow</fx-fill>
		</Variable>
		<Constant>
			<fx-fill>pink</fx-fill>
		</Constant>
		<Command>
			<fx-fill>green</fx-fill>
		</Command>
		<Text>
			<fx-fill>silver</fx-fill>	
		</Text>
		<Invalid>
			<fx-fill>silver</fx-fill>	
		</Invalid>
	</Word>
	
	<Windows>
		<Window>
			<fx-background-color>black</fx-background-color>
		</Window>
		<Window.content>
			<fx-background-color>black</fx-background-color>
			<fx-text-fill>silver</fx-text-fill>
		</Window.content>
	</Windows>
	
	<Rendering>
		<Render>
			<fx-background-color>red</fx-background-color>
		</Render>
		
		<Render>
			<turtle>red</turtle>
			<fx-fill>red</fx-fill>
			<fx-width>red</fx-width>
		</Render>
	</Rendering>
		
	<Colorpickering>
		<ColorPicker>
			<fx-font>8px 'Letter Gothic Std'</fx-font>
			<fx-background-color>white</fx-background-color>
		</ColorPicker>
	</Colorpickering>

</Colors>