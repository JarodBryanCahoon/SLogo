<?xml version="1.0" encoding="UTF-8"?>
<Colors>
	<Root>
		<fx-font>14px 'Letter Gothic Std'</fx-font>	
		<fx-text-fill>silver</fx-text-fill>
		<fx-background-color>black</fx-background-color>
		<fx-wrap-text>true</fx-wrap-text>
	</Root>
	
	
	<Keywords>
	
		<Variable>
			<fx-fill></fx-fill>
		</Variable>
		<Text>
			<fx-fill></fx-fill>	
		</Text>
		<Constant>
			<fx-fill></fx-fill>
		</Constant>
		<Command>
			<fx-fill></fx-fill>
		</Command>
		<Invalid>
			<fx-fill></fx-fill>	
		</Invalid>
		
	</Keywords>
	
	<Window>
	
		<fx-background-color></fx-background-color>
		
	</Window>
	
	<Render>
		<Background></Background>
		<Turtle></Turtle>
		<PenColor></PenColor>
		<PenWidth></PenWidth>
	</Render>
	
	
</Colors>