<?xml version="1.0" encoding="UTF-8"?>
<Colors>
	<root>
	<fx-font>14px 'Letter Gothic Std'</fx-font>	
	<fx-text-fill>silver</fx-text-fill>
	<fx-background-color>black</fx-background-color>
	<fx-wrap-text>true</fx-wrap-text>
	</root>



	<variable>
		<fx-fill>greenyellow</fx-fill>
	</variable>
	<text>
		<fx-fill>silver</fx-fill>	
	</text>
	<constant>
		<fx-fill>pink</fx-fill>
	</constant>
	<command>
		<fx-fill>orangered</fx-fill>
	</command>
	<invalid>
		<fx-fill>silver</fx-fill>	
	</invalid>
	

	<window>
		<fx-background-color>black</fx-background-color>
	</window>

	

	<render>
		<fx-background-color></fx-background-color>
		<turtle></turtle>
		<fx-fill></fx-fill>
		<fx-width></fx-width>
	</render>	
</Colors>